{
	//=============================================================
	//    DROPS parameter file for stokesP2.cpp
	//    simulation of stationary driven cavity problem
	//=============================================================
	// time stepping
	"Time":
	{ 
		"NumSteps":		50,
		"StepSize":		0.01,
		"Scheme":		1 	 //not used in the stationary case
	},

	// flow solver
	"Stokes":
	{ 
		"InnerIter":		10,
		"OuterIter":		200,
		"InnerTol":		0.6,
		"OuterTol":		1e-10,
		"StokesMethod":		30207,
		"PcAIter":		1, 	 //V-cycles multigrid, iter pc for A block
		"PcATol":		1e-99,
		"PcSTol":		0.01,
		"XFEMStab":		0,
		"Theta":		1 	 //backward Euler
	},

	// stokes coefficients
	"StokesCoeff":
	{ 
		"Reaction":		"ScaZero", 	 //Zero scalar function
		"Source":		"VecZero",
		"Solution_Vel":		"None",
		"Solution_DVel":		"None",
		"Solution_Pr":		"None"
	},

	// Miscellaneous
	"Misc":
	{ 
		"Omega":		1,
		"Tau":		1,
		"MarkLower":		0.25 	 //a double value, range from 0 to the maximum value of Y coordinate, mark all tetrahedron (y<MarkLower) for further refinement
	},

	// material data, all units are SI
	"Mat":
	{ 
		"Dens":		1,
		"Visc":		1
	},

	// experimental conditions
	"Exp":
	{ 
		"Gravity":		[0, 0, 0 ]
	},

	// domain, bnd/ini conditions
	"DomainCond":
	{ 
		"InitialCond":		2, 	 //initial refinement
		"GeomType":		1, 	 //specifies the used geometry (0=ReadMeshBuilder, 1=BrickBuilder, 2=CavityBuilder, 3=LBuilder, 4=BBuilder)
		"InitialFile":		"none", 	 //file prefix when reading data for initial condition
		"MeshFile":		"1x1x1@5x5x5", 	 //mesh file (created by GAMBIT, FLUENT/UNS format) or dimensions of a cuboid (e.g. 2x3x4\@5x6x7)
		"BoundaryType":		"0!0!0!0!0!2", 	 //Dir0BC= 0, DirBC= 2, Per1BC= 13, Per2BC= 11, Nat0BC= 21, NatBC= 23, NoBC= 98, UndefinedBC_= 99
		"BoundaryFncs":		"VecZero!VecZero!VecZero!VecZero!VecZero!Inflow_drivcav_v2" 	 //Two versions of inflow condition
	},

	// adaptive refinement
	"AdaptRef":
	{ 
		"Freq":		0,
		"FinestLevel":		0,
		"CoarsestLevel":		0,
		"Width":		0.1,
		"LoadBalStrategy":		1, 	 //0 - No Migration, 1 - Adaptive, 2 - Recursive
		"Partitioner":		1 	 //1 - Metis, 2 - Zoltan, 3 - Scotch
	},

	// Ensight settings
	"Ensight":
	{ 
		"EnsightOut":		1, 	 //Ensight output
		"EnsCase":		"cavity", 	 //name of Ensight Case
		"EnsDir":		"ensight", 	 //local directory for Ensight files
		"GeomName":		"cave", 	 //name for the geometry
		"MasterOut":		1, 	 //only master writes out ensight files
		"Binary":		1 	 //write out ensight files in binary format
	},

	// Error estimator    
	"Err":
	{ 
		"RelReduction":		0.1,
		"MinRatio":		0, 	 //not used in Stokes case
		"Threshold":		0.6,
		"Meas":		1,
		"DoMark":		0, 	 //1 = true, 0 = false
		"DoErrorEstimate":		0, 	 //1 = true, 0 = false
		"NumRef":		3 	 //also used for MarkLower refinement
	}

}