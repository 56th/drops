{

        //General section
        "General":
        {
                "ProgressBar":  1, //show progress bar
                "DynamicLibsPrefix": "../", //where should we look for shared objects?
                "DynamicLibs": [ //select libraries to be loaded after program start
                        "misc/libmisc-scalarFunctions",
                        "misc/libmisc-vectorFunctions",
                        "misc/libmisc-csgFunctions",
                        "osmosis/libosmosis-osmosisCoeff"
                ]
        },

        "Time":
        {
                "NumSteps":             50,
                "StepSize":             0.01,
                "Scheme":               0.5
        },
        
        "CSG":
        {
                "Library":  "osmosisCSG.json",
                "Geometry":  "interrupted sphere"
        },

        "Osmosis":
        {
                "Levelset":             "csg_fun",
                "ScaleInitialConc":          0,
                "InitialConc":          "linearConc",
                "Diffusivity":          0.25,
                "alpha":                0.25,
                "beta":                 1.0
        },      
        
        "Exp":
        {
                "RadDrop":              [1.0, 1.5, 1.0 ],
                "PosDrop":              [2.0, 2.0, 2.0 ],
                "Gravity":              [0, -9.81, 0 ],
                "OmitBound":            1e-4
        },  
        
        "Stabilisation":
        {
                "Active":               1,
                "Scaling":              4.0,
                "Exp":                  1.0,
                "Anisotropic":          1
        },
        
        "Solver":
        {
                "Iter":                 1000,
                "Tol":                  1e-16,
                "Interface":            2
        },
        
        "Coupling":
        {
                "Active":               0,
                "Damping":              0.5,
                "maxIter":              100,
                "Tol":                  1e-6
        },

        "Levelset":
        {
                "Tol":                  1e-10,
                "Iter":                 500,
                "SD":                   0.1,
                "CurvDiff":             -1,
                "VolCorrection":        0,
                "Theta":                1,
                "Discontinuous":        0       // Do not use DG for Levelset-Discretisation
        },
        
        "Reparam":
        {
                "Active":               0,
                "method":               13,
                "VolCorrection":        0
        },

        "AdaptRef":
        {
                "_comment":
"# adaptive refinement",

                "Freq":                 0,
                "FinestLevel":          0,
                "CoarsestLevel":        0,
                "Width":                0.1,
                "RefineStrategy":       1,
                "LoadBalStrategy":      1,
                "Partitioner":          1
        },

        "VTK":
        {
                "VTKOut":               1,
                "VTKDir":               "vtk",
                "VTKName":              "SimpleOsmosis",
                "Binary":               0
        },

        "GNUPlot":
        {
            "Out":                      0,
            "Plotname":                 "TestCase5/plot/CoupLSInth20t10.plot"
        },

        "DomainCond":
        {
                "_comment":
"# domain, bnd/ini conditions",

                "InitialCond":          0,
                "GeomType":             1,
                "InitialFile":          "none",
                "MeshFile":             "2x2x2@12x12x12",
                "BoundaryType":         "21!21!21!21!21!21!",
                "BoundaryFncs":         "ZeroVel!ZeroVel!ZeroVel!ZeroVel!ZeroVel!ZeroVel"
        }

}
