{
    "_comment":
"
#======================================================================
#    DROPS parameter file for prJump
#    simulation of the static droplet governed by the stokes equation
#    study the accuarcy and stability of various discretisations
#======================================================================
",
    "Stokes":
    {
        "_comment": "# flow solver",
        "StokesMethod": "10211",
        "XFEMStab": "0",

        "InnerIter": "1000",
        "OuterIter": "1000",
        "InnerTol": "0.6",
        "OuterTol": "1e-03",
        "PcAIter": "100",
        "PcATol": "0.01",
        "PcSTol": "0.01",
        "Theta": "1",
        "epsP": "0",
        "PrintMatrices": "false"
    },

    "AdaptRef":
    {
        "_comment": "# adaptive refinement",
        "Freq": "0",
        "FinestLevel": "3",
        "CoarsestLevel": "0",
        "Width": "1e-08",
        "LoadBalStrategy": "1011"
    },

    "Mat":
    {
        "_comment": "# material data, all units are SI",
        "DensDrop": "1",
        "ViscDrop": "1",
        "DensFluid": "1",
        "ViscFluid": "1",
        "SmoothZone": "1e-05",
        "ShearVisco": "0",
        "DilatationalVisco": "0"
    },

    "Exp":
    {
        "_comment": "# experimental conditions",
        "RadDrop":
        [
            "0.6666666",
            "0.6666666",
            "0.6666666"
        ],
        "PosDrop":
        [
            "0",
            "0",
            "0"
        ],
        "Gravity":
        [
            "0",
            "0",
            "0"
        ],
        "FlowDir": "0",
        "InflowVel": "0",
        "RadInlet": "0",
        "InflowFreq": "0",
        "InflowAmpl": "0",
        "VolForce": "ZeroVel"
    },

    "SurfTens":
    {
        "_comment": "# surface tension",
        "SurfTension": "1",
        "ArtificialForce": "0",
        "VarTension": "0",
        "JumpWidth": "0",
        "RelPos": "0",
        "DirtFactor": "0",
        "ShearVisco": "0",
        "DilatationalVisco": "0"
    },

    "VTK":
    {
        "_comment": "# VTK settings",
        "vtkOut": "true",
        "vtkCase": "prJump",
        "vtkDir": "~/dropsbin/prjump_test/vtk",
        "Binary": "0",
        "MasterOut": "1",
        "GeomName": "Cube"
    },

///////////////////////////////////////////////////////////////////////////
// Below here only general settings follow, which should not be modified //
// for this test case.                                                   //
///////////////////////////////////////////////////////////////////////////

    // Load dynamic libraries.
    "General":
    {
        "ProgressBar": "1",
        "DynamicLibsPrefix": "../",
        "DynamicLibs":
        [
            "levelset/liblevelset-twophaseCoeff",
            "misc/libmisc-scalarFunctions",
            "misc/libmisc-vectorFunctions"
        ]
    },

    // Disable timestepping, as we are solving a stationary problem.
    "Time":
    {
        "NumSteps": "0",
        "StepSize": "0",
        "Scheme": "0"
    },

    // Disable the Navier-Stokes solver, as we are interested only in te
    // Stokes part.
    "NavStokes":
    {
        "Nonlinear": "0",
        "Tol": "0",
        "Iter": "0",
        "Reduction": "0",
        "Downwind":
        {
            "Frequency": "0",
            "MaxRelComponentSize": "0.05",
            "WeakEdgeRatio": "0.2",
            "CrosswindLimit": "0.8660254037844387"
        }
    },

    // Disable the levelset solver, as the interface is stationary.
    "Levelset":
    {
        "_comment": "# levelset solver",
        "Tol": "0",
        "Iter": "0",
        "SD": "0",
        "CurvDiff": "0",
        "VolCorrection": "0",
        "Theta": "0",
        "Downwind":
        {
            "Frequency": "0",
            "MaxRelComponentSize": "0.05",
            "WeakEdgeRatio": "0.2",
            "CrosswindLimit": "0.8660254037844387"
        }
    },

    // Thus no coupling is required.
    "Coupling":
    {
        "Tol": "0",
        "Iter": "0",
        "Stab": "0",
        "Projection": "0"
    },

    // No reinitialisation either.
    "Reparam":
    {
        "_comment": "# re-initialization of levelset function",
        "Freq": "0",
        "Method": "0",
        "MinGrad": "0",
        "MaxGrad": "0",
        "NarrowBand": "0"
    },

    // Do not solve the surfactant equation.
    "Transp":
    {
        "_comment": "# mass transport",
        "DoTransp": "0",
        "Theta": "0",
        "Iter": "0",
        "Tol": "0",
        "DiffPos": "0",
        "DiffNeg": "0",
        "HPos": "0",
        "HNeg": "0",
        "IniCPos": "0",
        "IniCNeg": "0",
        "NitschePenalty": "0",
        "NitscheXFEMStab": "0"
    },

    // No surfactants...
    "SurfTransp":
    {
        "_comment": "# surfactant transport",
        "DoTransp": "0",
        "Theta": "0",
        "Iter": "0",
        "Tol": "0",
        "OmitBound": "0",
        "Visc": "0"
    },

    // No restart, stationary problem...
    "Restart":
    {
        "_comment": "# Write out results, read in for restart",
        "Serialization": "0",
        "Overwrite": "0",
        "Inputfile": "0",
        "Outputfile": "0",
        "Binary": "0"
    },

    // Zero "initial" conditions. Do not really matter,
    // we still have a stationary problem.
    "DomainCond":
    {
        "_comment": "# domain, bnd/ini conditions",
        "InitialCond": "1",
        "GeomType": "0",
        "InitialFile": "0",
        "MeshFile": "4",
        "BoundaryType": "0",
        "BoundaryFncs": "0"
    }
}

