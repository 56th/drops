{
//=============================================================
//    DROPS parameter file
//    Study different discretizations of interfacial tension
//=============================================================
    "General": 
    {
        "DynamicLibs": 
        [
            "misc/libmisc-scalarFunctions", 
            "misc/libmisc-vectorFunctions"
        ], 
        "DynamicLibsPrefix": "../", 
        "ProgressBar": 1
    }, 
    
    
    "CouplingSolver": 
    {
        "LevelsetSolver": 
        {
            "Iter": 10000, 
            "Tol": 1e-14
        }
    },
    
    
    "Levelset": 
    {
        "PosDrop": [ 0, 0, 0 ], 
        "Discontinuous": 0, 
        "BndData": { "Default": [ "NoBC" ] }, 
        "RadDrop": [ 0.5, 0.5, 0.5 ], 
        "VolCorrection": 0, 
        "SD": 0,
        "CurvDiff": -5e-09
    }, 
    
    "Deprecated": 
    {        
        "Theta": 1
    },
    
    
    "Inflow": 
    {
        "RadInlet": 0.0035
    }, 
    
    
    "Mesh": 
    {
        "Type": "BrickBuilder", 
        "Origin": [ -1, -1, -1 ], 
        "N1": 5, 
        "N2": 5, 
        "N3": 5, 
        "E1": [ 2, 0, 0 ], 
        "E3": [ 0, 0, 2 ], 
        "E2": [ 0, 2, 0 ],
        "AdaptRef": 
        {
            "CoarsestLevel": 0, 
            "FinestLevel": 2, 
            "Width": 0.00049
        }
    }, 
    
    
    "NavStokes": 
    {
        "Coeff": 
        {
            "SurfTens": 
            {
                "SurfTension": 1, 
                "VarTensionFunc": "ConstTau"
            }
        },
        
        "BoundaryData":
        {
            "Velocity": { "Default": [ "DirBC", "VecZero" ] }
        }
    }
    
}
