{
    "Output": {
      "Directory":  "../../output/tmp",
      "EveryStep":  1,
      "Binary":     true,
      "Velocity": false,
      "Vorticity": true,
      "SurfSpeed": false,
      "Pressure": false
    },

    "Mesh": {
        "Type": "BrickBuilder",
        "Origin": [-1.66666,-1.66666,-1.66666],
        "E1": [3.33333, 0,0],
        "E2": [0,3.33333,0],
        "E3": [0,0,3.33333],
        "N1": 2,
        "N2": 2,
        "N3": 2,
        "RestartFile": "",
        "AdaptRef": {
            "Comment":      "adaptive refinement",
            "Freq":          0,
            "Width":         0,
            "CoarsestLevel": 0,
            "FinestLevel":   4,
            "SD":            0.1,
            "CurvDiff":      -1,
            "Discontinuous": 0
        }
    },

    "SurfNSCH": {
      "BDF":                      2,
      "NumbOfVirtualSubEdges":    2,
      "ExportMatrices":           false,
      "ExportMatricesFormat":     "mat",
      "IC" : {
        "Comment" : "initial condition parameters",
        "KelvinHelmholtzCristoph" : {
          "Delta_0" : 0.05,
          "cn" : 0.01,
          "aa" : 1.0,
          "ma" : 16.0,
          "ab" : 0.1,
          "mb" : 20.0
        },
        "TorusVarTube" : {
          "R"   : 1.0,
          "r_0" : 0.3,
          "r_1" : 0.5
        }
      },
      "rho" : {
        "max" : 1.0,
        "min" : 1.0
      },
      "NS" : {
        "gamma": 1.0,
        "nu": 1e-5,
        "UseTangentialMassMatrix": false,
        "TestName": "KelvinHelmholtzCristophSphere",
        "PressureVolumestabType": "Normal",
        "NormalPenaltyPower": -2,
        "NormalPenaltyFactor": 1,
        "VelocityVolumestabPower": -1,
        "VelocityVolumestabFactor": 1,
        "PressureVolumestabPower": 1,
        "PressureVolumestabFactor": 1,
        "Formulation": "Consistent"
      },
      "CH" : {
        "Epsilon": 0.01,
        "MobilityScaling": 1.0,
        "c_0" : 0.5,
        "ChemicalPotentialScaling": 1.0,
        "Beta_s": 1.0,
        "UseDegenerateMobility": false
      }
    },

    "Time": {
      "Read":         0,
      "Write":        "yes",
      "NumbOfSteps":  20,
      "FinalTime":    20.0
    },

    "Solver": {
        "UsePreviousFrameAsInitialGuess" : false,
        "Outer" : {
          "Iteration" : "FLEXIBLE GMRES",
          "OutputFrequency" : 1,
          "KrylovSubspaceSize" : 100,
          "MaxIter" : 50,
          "RelResTol" : 1e-8
        },
        "Inner" : {
            "Use" : true,
            "Type" : "BlockTriangular",
            "A" : {
              "Comment" : "use Iteration = Amesos2_* (e.g. Amesos2_Klu, Amesos2_SuperLU etc.) for the sparse-direct; Type = Full for full factorization, BlockTriangular for block Gauss-Seidel, BlockDiagonal for block Jacobi",
              "Iteration" : "Amesos2_KLU",
              "Type" : "Full"
            },
            "S" : {
              "S_L" : true,
              "Iteration" : "CG",
              "MaxIter" : 200,
              "RelResTol" : 1e-2
            }
        }
    },

    "Levelset": {
      "Theta":         1,
      "VolCorrection": 0,
      "SD":            0.1,
      "CurvDiff":      -1,
      "Discontinuous": 0,
      "BndData": {
        "Default": ["NoBC"], "0" : ["NoBC"], "1" : ["NoBC"], "2" : ["NoBC"], "3" : ["NoBC"], "4" : ["NoBC"], "5" : ["NoBC"]
      },
      "Solver": {
        "Tol":  1e-10,
        "Iter": 500
      },
      "ShiftNorm": 0.0,
      "ShiftDir":  [1, 1, 1]
    },

    "General": {
      "ProgressBar":  1,
      "DynamicLibsPrefix": "../",
      "DynamicLibs": []
    },

    "Stokes": {
      "VelocityBndData":       { "Default": ["NoBC"], "0": ["NoBC"], "1": ["NoBC"], "2": ["NoBC"], "3": ["NoBC"], "4": ["NoBC"], "5": ["NoBC"] },
      "PressureBndData":       { "Default": ["NoBC"], "0": ["NoBC"], "1": ["NoBC"], "2": ["NoBC"], "3": ["NoBC"], "4": ["NoBC"], "5": ["NoBC"] },
      "VolumeFractionBndData": { "Default": ["NoBC"], "0": ["NoBC"], "1": ["NoBC"], "2": ["NoBC"], "3": ["NoBC"], "4": ["NoBC"], "5": ["NoBC"] },
      "ChemPotentialBndData":  { "Default": ["NoBC"], "0": ["NoBC"], "1": ["NoBC"], "2": ["NoBC"], "3": ["NoBC"], "4": ["NoBC"], "5": ["NoBC"] }
    },

    "Reparam": {
        "Comment": "re-initialization of levelset function",
        "Freq":		0,
        "Method":	1
    },

    "Exp": {
        "Comment": "experimental conditions",
        "RadDrop":		[1, 1, 1 ],
        "PosDrop":		[0, 0, 0 ],
        "Velocity":		[0, 0, 0.5 ]
    },

    "SurfTransp": {
        "Comment":  "surfactant transport",
        "Theta":     0.5,
        "OmitBound": -1,
        "Visc":      0.05,
        "FEdegree":  2
    },

    "TestCase":         1,
    "InitialDivisions": 2,
    "EnsightCase":      "none",
    "EnsightDir":       "CNmass-lvl3",
    "LB":               1
}